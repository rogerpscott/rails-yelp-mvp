<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.error_notification %>

    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"], prompt: "Select a category", selected: 1 %>


    <%= f.button :submit %>
  <% end %>
</div>
